{
  "version": "1.0.6",
  "description": "A component to allow the ASP.NET Core data protection stack to work with the ASP.NET 4.x <machineKey> element.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/dataprotection"
    },
    "tags": [
      "aspnet",
      "aspnetcore",
      "dataprotection"
    ],
    "files": {
      "mappings": {
        "content/net451/": "web.config.transform"
      }
    }
  },
  "frameworks": {
    "net451": {
      "dependencies": {
        "Microsoft.AspNetCore.DataProtection": "1.0.6",
        "Microsoft.Extensions.DependencyInjection": "1.0.3"
      },
      "frameworkAssemblies": {
        "System.Configuration": "4.0.0.0",
        "System.Security": "4.0.0.0",
        "System.Web": "4.0.0.0"
      }
    }
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  }
}